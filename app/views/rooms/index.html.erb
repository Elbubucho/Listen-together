<div id="chatroom_container" class="flex flex-wrap">
  <div class="w-full sm:w-1/3 md:w-1/3 px-2 pb-5">
    <h4 class="font-bold text-2xl text-center"> Messages</h4>

    <%= turbo_stream_from "users" %>

    <div id="users" class="p-5 gap-5" data-controller="user-lis">
      <%= render "users/users", users: @users, single_room: @single_room %>
    </div>
  </div>
</div>
